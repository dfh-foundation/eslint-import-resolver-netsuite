{
  "name": "@dfh-foundation/eslint-import-resolver-netsuite",
  "description": "eslint-plugin-import resolver for NetSuite projects",
  "version": "1.1.0",
  "author": "Johnny Walker <jwalker@designsforhealth.com>",
  "contributors": [],
  "dependencies": {
    "debug": "^4.3.4",
    "resolve": "^1.19.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@tsconfig/recommended": "^1.0.2",
    "@types/chai": "^4.3.4",
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "@types/resolve": "^1.20.2",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "chai": "^4.3.7",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "source-map-support": "^0.5.21",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  },
  "keywords": [
    "eslint-plugin-import",
    "netsuite",
    "suitescript",
    "resolver"
  ],
  "license": "ISC",
  "main": "lib",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dfh-foundation/eslint-import-resolver-netsuite.git"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "lint": "eslint . && prettier --check .",
    "prepare": "tsc && husky install",
    "prepublishOnly": "yarn test",
    "test": "jest"
  }
}
